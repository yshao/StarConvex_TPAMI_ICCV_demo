ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda/bin/nvcc

cflags = -pthread -B /opt/conda/envs/py3/compiler_compat -Wl,--sysroot=/ -Wsign-compare -DNDEBUG -g -fwrapv -O3 -Wall -Wstrict-prototypes -fPIC -I/opt/conda/envs/py3/lib/python3.8/site-packages/torch/include -I/opt/conda/envs/py3/lib/python3.8/site-packages/torch/include/torch/csrc/api/include -I/opt/conda/envs/py3/lib/python3.8/site-packages/torch/include/TH -I/opt/conda/envs/py3/lib/python3.8/site-packages/torch/include/THC -I/usr/local/cuda/include -I/opt/conda/envs/py3/include/python3.8 -c
post_cflags = -g -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=pointnet2_cuda -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++14
cuda_cflags = -I/opt/conda/envs/py3/lib/python3.8/site-packages/torch/include -I/opt/conda/envs/py3/lib/python3.8/site-packages/torch/include/torch/csrc/api/include -I/opt/conda/envs/py3/lib/python3.8/site-packages/torch/include/TH -I/opt/conda/envs/py3/lib/python3.8/site-packages/torch/include/THC -I/usr/local/cuda/include -I/opt/conda/envs/py3/include/python3.8 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -O2 -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=pointnet2_cuda -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_80,code=compute_80 -gencode=arch=compute_80,code=sm_80 -std=c++14
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags



build /workspace/program/learning3d/utils/lib/build/temp.linux-x86_64-3.8/src/pointnet2_api.o: compile /workspace/program/learning3d/utils/lib/src/pointnet2_api.cpp
build /workspace/program/learning3d/utils/lib/build/temp.linux-x86_64-3.8/src/ball_query.o: compile /workspace/program/learning3d/utils/lib/src/ball_query.cpp
build /workspace/program/learning3d/utils/lib/build/temp.linux-x86_64-3.8/src/ball_query_gpu.o: cuda_compile /workspace/program/learning3d/utils/lib/src/ball_query_gpu.cu
build /workspace/program/learning3d/utils/lib/build/temp.linux-x86_64-3.8/src/group_points.o: compile /workspace/program/learning3d/utils/lib/src/group_points.cpp
build /workspace/program/learning3d/utils/lib/build/temp.linux-x86_64-3.8/src/group_points_gpu.o: cuda_compile /workspace/program/learning3d/utils/lib/src/group_points_gpu.cu
build /workspace/program/learning3d/utils/lib/build/temp.linux-x86_64-3.8/src/interpolate.o: compile /workspace/program/learning3d/utils/lib/src/interpolate.cpp
build /workspace/program/learning3d/utils/lib/build/temp.linux-x86_64-3.8/src/interpolate_gpu.o: cuda_compile /workspace/program/learning3d/utils/lib/src/interpolate_gpu.cu
build /workspace/program/learning3d/utils/lib/build/temp.linux-x86_64-3.8/src/sampling.o: compile /workspace/program/learning3d/utils/lib/src/sampling.cpp
build /workspace/program/learning3d/utils/lib/build/temp.linux-x86_64-3.8/src/sampling_gpu.o: cuda_compile /workspace/program/learning3d/utils/lib/src/sampling_gpu.cu





